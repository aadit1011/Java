public class CaseInsensitive {

	public static void main(String[] args) {
		CaseInsensitive case1=new CaseInsensitive();
		case1.caseChecker("Bardan", "BARdan");
		case1.caseChecker("Aadit", "Aadtiys");
	}
	public void caseChecker(String str1,String str2)
	{
		if(str1.toLowerCase().equals(str2.toLowerCase()))
		{
			System.out.println("True");
		}
		else
		{
			System.out.println("False");
		}
	}

}
